def fibonacci(n):
    a, b = 0,1
    if n == 0:
        return True
    while b<n:
        a,b= b,a+b
    return b == n

numero = 5
if fibonacci(numero):
    print(f"O número {numero} pertence a sequência de Fibonacci.")
else:
    print(f"O número {numero} não faz parte a sequência.")

//Código feito na linguagem Python/Pycharm
